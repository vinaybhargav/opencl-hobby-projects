__kernel void matrix_mul( __global int* matA, __global int* matB , __global int* matC, int matA_col, int matB_col){   int x = get_global_id(0);   int y = get_global_id(1);   int value = 0;   for (int i = 0; i < matA_col; ++i){   int elementA = matA[y * matA_col + i];      int elementB = matB[i * matB_col + x];      value += elementA * elementB;   }   matC[y * matB_col + x] = value;}
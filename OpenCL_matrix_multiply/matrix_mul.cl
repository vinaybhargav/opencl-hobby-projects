__kernel void matrix_mul( __global int* matA, __global int* matB , __global int* matC, __global int matA_col, __global int matB_col){   int x = get_global_id(0);   int y = get_global_id(1);   __local int value = 0;   __local int i = 0;   for (i = 0; i < matA_col; ++i){   __local int elementA = matA[y * matA_col + i];      __local int elementB = matB[i * matB_col + x];      value += elementA * elementB;   }   matC[y * matB_col + x] = value;}
